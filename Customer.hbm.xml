<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="com.cte.pmo.Customer" optimistic-lock="version" polymorphism="implicit" select-before-update="false"/>
        <id name="customerId" column="CUSTOMER_ID"></id>
		<property name="name"  column="NAME" />
		<property name="age"  column="AGE" />
		<property name="email"  column="EMAIL" />
        <property name="telephone"  column="PHONE_NUMBER" />

</hibernate-mapping>
